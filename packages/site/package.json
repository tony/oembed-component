{
  "name": "@social-embed/site",
  "version": "0.0.1-next.0",
  "license": "MIT",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "TYPEDOC_WATCH=true docusaurus start --port 8063",
    "prebuild": "npm run clean && yarn workspace @social-embed/lib build && npm run docs:oembed && yarn api",
    "build": "TYPEDOC_WATCH=false docusaurus build --out-dir dist --no-minify",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clean:typegen": "rimraf -g './docs/**/api'",
    "clean:oembed": "rimraf -g static/js/o-embed.bundled.js",
    "clean": "yarn clean:typegen && yarn clean:oembed && docusaurus clear",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve --dir dist --port 8062",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "docs:oembed": "yarn workspace @social-embed/wc build && rollup -c --file static/js/o-embed.bundled.js",
    "lint": "echo 'NOOP'",
    "test": "echo 'NOOP'",
    "ncu": "ncu",
    "update": "ncu",
    "api:wc": "yarn workspace @social-embed/wc run extract-api; yarn run api-documenter markdown --input-folder ../wc/temp --output docs/wc/api",
    "api:lib": "yarn workspace @social-embed/lib run extract-api; yarn run api-documenter markdown --input-folder ../lib/temp --output docs/lib/api",
    "api": "yarn run api:wc; yarn run api:lib"
  },
  "dependencies": {
    "@docusaurus/core": "2.0.0-beta.3",
    "@docusaurus/preset-classic": "2.0.0-beta.3",
    "@docusaurus/theme-live-codeblock": "^2.0.0-beta.3",
    "@mdx-js/react": "^1.6.22",
    "@microsoft/api-documenter": "^7.13.24",
    "clsx": "^1.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.14.5",
    "@docusaurus/module-type-aliases": "^2.0.0-beta.3",
    "@tsconfig/docusaurus": "^1.0.2",
    "@types/react": "^17.0.14",
    "@types/react-helmet": "^6.1.2",
    "@types/react-router-dom": "^5.1.8",
    "rollup-plugin-node-resolve": "^5.2.0"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
